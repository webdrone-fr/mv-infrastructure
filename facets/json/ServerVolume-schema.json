{
    "storages": ["SQL"],
    "default": "Location for storing data in Instance",
    "$schema": "http://json-schema.org/draft-07/schema",
    "id": "ServerVolume",
    "title": "Server Volume",
    "type": "object",
    "properties": {
        "server": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Server volume is attached to",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_server",
            "title": "ServerVolume.server",
            "type": "string",
            "maxLength": 255
        },
        "volumeType": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Volume Type",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_volumeType",
            "title": "ServerVolume.volumeType",
            "enum": [
                "b_ssd",
                "l_ssd"
            ]
        },
        "zone": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Zone",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_zone",
            "title": "ServerVolume.zone",
            "enum": [
                "fr-par-1",
                "fr-par-2",
                "fr-par-3",
                "pl-waw-1",
                "nl-ams-1"
            ]
        },
        "name": {
            "storages": ["SQL"],
            "nullable": true,
            "description": "Name of Volume",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_name",
            "title": "ServerVolume.name",
            "type": "string",
            "maxLength": 255
        },
        "state": {
            "storages": ["SQL"],
            "default": "available",
            "nullable": true,
            "description": "State",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_state",
            "title": "ServerVolume.state",
            "enum": [
                "saving",
                "hotsyncing",
                "available",
                "resizing",
                "snapshotting",
                "fetching",
                "error"
            ]
        },
        "creationDate": {
            "storages": ["SQL"],
            "nullable": true,
            "format": "date-time",
            "description": "Creation Date",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_creationDate",
            "title": "ServerVolume.creationDate",
            "type": "string"
        },
        "isBoot": {
            "storages": ["SQL"],
            "default": "false",
            "nullable": true,
            "description": "Is used for server boot",
            "readOnly": false,
            "versionable": false,
            "id": "CE_ServerVolume_isBoot",
            "title": "ServerVolume.isBoot",
            "type": "string"
        }
    }
}